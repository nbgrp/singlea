{
    "name": "nbgrp/singlea-redis-bundle",
    "description": "SingleA Redis storage handlers",
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Alexander Menshchikov",
            "email": "alexander.menshchikov@yandex.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "nbgrp/singlea-feature-config-contracts": "^0.1",
        "nbgrp/singlea-marshaller-contracts": "^0.1",
        "nbgrp/singlea-persistence-contracts": "^0.1",
        "psr/log": "^3",
        "snc/redis-bundle": "^3.6 || ^4",
        "symfony/dependency-injection": "^6",
        "symfony/http-foundation": "^6",
        "symfony/http-kernel": "^6"
    },
    "require-dev": {
        "predis/predis": "^1",
        "symfony/cache": "^6"
    },
    "provide": {
        "nbgrp/singlea-persistence-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "SingleA\\Bundles\\Redis\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "endpoint": [
                "https://api.github.com/repos/nbgrp/singlea-recipes/contents/index.json",
                "flex://defaults"
            ]
        }
    }
}
