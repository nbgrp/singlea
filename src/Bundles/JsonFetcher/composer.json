{
    "name": "nbgrp/singlea-json-fetcher-bundle",
    "description": "SingleA external payload fetcher JSON implementation",
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Alexander Menshchikov",
            "email": "alexander.menshchikov@yandex.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "nbgrp/singlea-payload-fetcher-contracts": "^0.1",
        "symfony/config": "^6",
        "symfony/dependency-injection": "^6",
        "symfony/http-client-contracts": "^3",
        "symfony/http-client-implementation": "^3",
        "symfony/http-kernel": "^6"
    },
    "provide": {
        "nbgrp/singlea-payload-fetcher-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "SingleA\\Bundles\\JsonFetcher\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "endpoint": [
                "https://api.github.com/repos/nbgrp/singlea-recipes/contents/index.json",
                "flex://defaults"
            ]
        }
    }
}
